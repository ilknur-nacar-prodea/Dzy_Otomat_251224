<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:ndc="sap.ndc"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:fiori="sap.ui.webc.fiori">
	<Dialog title="{i18n>SelectCountingType}" resizable="true" draggable="true">
		<VBox class="sapUiSmallMargin">
			<Button text="{i18n>VehicleCounting}" press="onAracSayimKaydet" type="Emphasized" width="300px"/>
			<Button text="{i18n>AutomatCounting}" press="onOtomatSayimKaydet" type="Emphasized" width="300px"/>
		</VBox>
		<endButton>
			<Button text="Kapat" press="onCloseCountingType" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>